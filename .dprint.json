{
	"lineWidth": 100,
	"useTabs": true,
	"markdown": {
		"textWrap": "always",
		"emphasisKind": "asterisks",
		"newLineKind": "system"
	},
	"toml": {
		"newLineKind": "system"
	},
	"includes": [
		"**/*.{md,json,toml}"
	],
	"plugins": [
		"https://plugins.dprint.dev/markdown-0.15.1.wasm",
		"https://plugins.dprint.dev/json-0.17.0.wasm",
		"https://plugins.dprint.dev/toml-0.5.4.wasm"
	]
}
